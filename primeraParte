public class Main {
    public static void main(String[] args) {
        System.out.println("Hello world!");

        int resultant = sumaTresnumeros(10, 20, 30);
        System.out.println(resultant);

    }

    public static int sumaTresnumeros(int a, int b, int c) {

        return a+b+c;


    }


}
